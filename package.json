{
  "name": "WikipediaNavigator",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "postinstall": "npm run build:client & npm run build:server",
    "start": "node dest/app.js",
    "start:dev": "npm run watch:server & npm run watch:client",
    "build:server": "babel src/server --out-dir dest",
    "build:client": "browserify src/client/app.js --transform babelify --outfile public/main.js",
    "watch:server": "babel-watch src/server/app.js",
    "watch:client": "watchify --verbose --debug src/client/app.js --transform babelify --outfile public/main.js"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-core": "^6.26.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babelify": "^8.0.0",
    "body-parser": "~1.13.2",
    "browserify": "^15.0.0",
    "cheerio": "^1.0.0-rc.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "dotenv": "^4.0.0",
    "express": "~4.13.1",
    "googleapis": "^2.1.6",
    "material-ui": "^0.20.0",
    "mongodb": "^2.0.48",
    "morgan": "~1.6.1",
    "mysql": "^2.15.0",
    "mysql2": "^1.5.1",
    "node-bing-api": "^3.2.2",
    "nodemw": "~0.7.5",
    "react": "^0.14.9",
    "react-autobind": "^1.0.6",
    "react-autosuggest": "^9.3.4",
    "react-bootstrap": "^0.28.1",
    "react-dom": "^0.14.3",
    "react-youtube": "^4.1.2",
    "request": "^2.65.0",
    "serve-favicon": "~2.3.0",
    "superagent": "^3.8.2",
    "underscore": "^1.8.3",
    "webworkify": "^1.5.0",
    "youtube-api": "^1.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-watch": "^2.0.7",
    "watchify": "^3.9.0"
  }
}
